{
    "name": "linna/filter",
    "type": "library",
    "description": "Linna Filter",
    "keywords": ["form", "validator", "php", "filter"],
    "homepage": "https://github.com/linna/filter",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastian Rapetti",
            "email": "sebastian.rapetti@tim.it"
        }
    ],
    "support": {
        "issues": "https://github.com/linna/filter/issues"
    },
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "infection/infection": "^0.12",
        "phpstan/phpstan": "^0.11",
